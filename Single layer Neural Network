      "source": [
        "import torch\n",
        "\n",
        "def activation(x):\n",
        "  return 1/(1+torch.exp(-x))\n",
        "\n",
        "torch.manual_seed(7)\n",
        "features=torch.randn((1,5))\n",
        "weights=torch.randn_like((features))\n",
        "bias=torch.randn((1,1))\n",
        "\n",
        "y=activation(torch.mm(features,weights.view(5,1))+bias)\n",
        "print(y)"
      ],
